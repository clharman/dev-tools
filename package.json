  "scripts": {
    "lint": "eslint . --fix",
    "dev": "NODE_ENV=development tsx -r dotenv/config server/index.ts",
    "local": "NODE_ENV=development tsx -r dotenv/config --watch server/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:ui": "playwright test tests/e2e/auth.spec.ts tests/e2e/core-workflows.spec.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/.*\\.test\\.js$ --coverage",
    "test:integration": "jest tests/background-job-integration.test.js",
    "test:background": "jest tests/background-job-integration.test.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
